{
  "name": "oh-my-live2d-root",
  "private": true,
  "version": "0.7.1",
  "scripts": {
    "build": "pnpm -F oh-my-live2d build",
    "build:docs": "pnpm build && node ./scripts/typedoc.mjs && pnpm -F oh-my-live2d-docs docs:build",
    "dev:docs": "concurrently \"node ./scripts/typedoc.mjs -w\" \"pnpm -F oh-my-live2d-docs docs:dev\"",
    "refresh": "node ./scripts/refresh-cdn.mjs",
    "release": "pnpm -F oh-my-live2d build && pnpm refresh",
    "release:vuepress": "pnpm -F vuepress-plugin-oh-my-live2d run release",
    "test": "pnpm -F vite-app run dev",
    "test:vite": "pnpm -F vue-app run dev",
    "test:webpack": "pnpm -F react-app run start"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "axios": "^1.6.7",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "json2md": "^2.0.1",
    "release-it": "^17.1.1",
    "rimraf": "^5.0.5",
    "typedoc": "0.25.9",
    "typedoc-plugin-markdown": "3.17.1",
    "typescript": "^5.3.3",
    "vite": "5.1.4"
  },
  "packageManager": "pnpm@8.15.4"
}
