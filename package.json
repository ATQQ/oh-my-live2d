{
  "name": "oh-my-live2d",
  "version": "0.5.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.min.js",
  "module": "./dist/index.js",
  "scripts": {
    "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\" \"cross-env OML_ENV=dev node ./scripts/build.mjs\" \"npm run test\")",
    "build": "rimraf dist && tsc && tsc-alias && node ./scripts/build.mjs",
    "refresh": "node ./scripts/refresh-cdn.mjs",
    "release": "dotenv release-it && npm run refresh",
    "release:vuepress": "pnpm -F vuepress-plugin-oh-my-live2d run release",
    "test": "pnpm -F vite-app run dev",
    "test:vite": "pnpm -F vue-app run dev",
    "test:webpack": "pnpm -F react-app run start",
    "dev:docs": "concurrently \"node ./scripts/typedoc.mjs\" \"vitepress dev docs\"",
    "build:docs": "npm run build && node ./scripts/typedoc.mjs && vitepress build docs",
    "preview:docs": "vitepress preview docs"
  },
  "license": "MIT",
  "author": "loclink",
  "description": "live2d component for web. 看板娘组件, 支持model2、model3、model4",
  "keywords": [
    "Live2D",
    "cubism2",
    "cubism4"
  ],
  "homepage": "https://github.com/oh-my-live2d/oh-my-live2d#readme",
  "bugs": "https://github.com/oh-my-live2d/oh-my-live2d/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oh-my-live2d/oh-my-live2d.git"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vuepress/client": "2.0.0-beta.60",
    "axios": "^1.2.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.30.0",
    "fs-extra": "^11.1.0",
    "json2md": "^2.0.1",
    "pixi-live2d-display": "0.4.0",
    "pixi.js": "6.5.2",
    "release-it": "^15.5.1",
    "rimraf": "^3.0.2",
    "tianjie": "^0.0.8",
    "tsc-alias": "^1.8.8",
    "typedoc": "0.24.8",
    "typedoc-plugin-markdown": "3.15.4",
    "typescript": "^4.9.4",
    "vite": "5.1.4",
    "vitepress": "1.0.0-rc.44",
    "vue": "^3.4.19"
  }
}
